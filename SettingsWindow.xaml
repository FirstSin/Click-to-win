<Window x:Class="Game.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Game"
        mc:Ignorable="d"
        Title="SettingsWindow" Height="450" Width="800" WindowStyle="None" AllowsTransparency="True" WindowStartupLocation="CenterScreen" MouseEnter="SettingsWindow_OnMouseEnter">
    <Window.Background>
        <ImageBrush ImageSource="Images\MainBackground.jpg"></ImageBrush>
    </Window.Background>
    <Grid x:Name="ClickSoundChecker">
        <TextBlock Text="Settings" Foreground="White" Height="Auto" FontSize="40" Margin="335,10,317,382" Width="148" HorizontalAlignment="Center"></TextBlock>
        <Line X1="650" Y1="0"  Stroke="White" Margin="68,68,79,371"></Line>
        <TextBlock Margin="274,143,255,237" Text="In working..." Foreground="White" FontSize="50" HorizontalAlignment="Center" Width="271"></TextBlock>
        <Button Width="100" Height="30" Style="{StaticResource ButtonStyle}" Canvas.Left="990" Canvas.Top="15" Content="Back" FontSize="18" Click="Back_Click" Margin="355,410,345,10"></Button>
    </Grid>
</Window>
